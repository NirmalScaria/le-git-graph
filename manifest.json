{
  "name": "GitHub Tree Graph",
  "description": "An informative Git Commit graph to your github!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts" : [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["js/addCommitsButton.js", "js/openCommitsTab.js", "js/main.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["tabs", "*://*.github.com/", "storage", "activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "/images/github_tree_graph16.png",
      "32": "/images/github_tree_graph32.png",
      "48": "/images/github_tree_graph48.png",
      "128": "/images/github_tree_graph128.png"
    }
  },
  "icons": {
    "16": "/images/github_tree_graph16.png",
    "32": "/images/github_tree_graph32.png",
    "48": "/images/github_tree_graph48.png",
    "128": "/images/github_tree_graph128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["html/branchSelection.html"],
      "matches": ["<all_urls>"]
    }
  ]
}